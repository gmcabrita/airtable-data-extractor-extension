{
  "scripts": {
    "build": "node utils/build.js",
    "build:release": "NODE_ENV=production npm run build",
    "start": "node utils/webserver.js",
    "fmt:check": "npx prettier src",
    "fmt": "npx prettier --write src utils webpack.config.js",
    "dist": "rm -f extension.zip && npm run build:release && cd build && zip -r ../extension.zip * && cd -"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "fs-extra": "^9.0.0",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^4.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "lint-staged": {
    "*.{json,js,jsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "papaparse": "^5.2.0"
  }
}
